<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{yyyy-MM-dd} | %d{HH:mm:ss.SSS} | %-9.9t | %-5p | %-27.27logger{27} | %m%n</pattern>
        </encoder>
    </appender>
    <root level="warn">
        <appender-ref ref="STDOUT"/>
    </root>
    <logger name="loci.formats" level="off"/>
    <logger name="ome.formats" level="off"/>
</configuration>